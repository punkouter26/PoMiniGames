  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "FeatureFlags": {
    "EnableSwagger": true,
    "EnableDiagnostics": true
  },
  "AllowedHosts": "*"
}
