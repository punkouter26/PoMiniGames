{
  "KeyVault": {
    "Uri": ""
  },
  "Sqlite": {
    "DataDirectory": "data"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5000",
      "https://localhost:5001",
      "http://localhost:5173"
    ]
